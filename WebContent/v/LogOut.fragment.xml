<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Dialog
    title="LOG OUT"
    class="logOutPop" >
    <content>
      <Text width="100%" textAlign="Center" text="Are you sure you want to logout?" class="logouttext" />
    </content>
    <beginButton>
      <Button text="Yes" press="LogOutButton" />
    </beginButton>
    <endButton>
      <Button text="Cancel" press="LogOutCancelButton" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>